#include <iostream> //header file
using namespace std; //

int main(){
	int num=0;
	
	cout << "숫자를 입력하세요." << endl;
		//'endl' is as same as '\n'.
	
	cin >>num;
		//'cin >>' means computer is waiting for keyborad input.
	
	cout <<"the number is "<< num<< endl;
		//this code print the num.
		
	const int num1=5;
		//if you use const, you can't change the variable.
		
	
}
